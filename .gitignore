# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore

# Windows
[Dd]esktop.ini
[Tt]humbs.db
*.DS_Store

# Folder config file
*.sublime-workspace

# Autogenerated VS/MD solution and project files
*.csproj
*.unityproj
*.sln
*.suo
*.tmp
*.user
*.userprefs
*.pidb
*.booproj
*.svd

# Unity3D generated meta files
*.pidb.meta
*.pdb.meta
*.mdb.meta

# Unity3D Generated File On Crash Reports
sysinfo.txt

# Builds
[Bb]uilds/
[Bb]uild/

# Allow these directories to exist, but only include specific files
[Ll]ibrary/
[Ll]ibrary/ArtifactDB/
[Ll]ibrary/Artifacts/
[Ll]ibrary/metadata/
[Ll]ibrary/PlayerDataCache/
[Ll]ibrary/ScriptAssemblies/
[Ll]ibrary/StateCache/
[Ll]ibrary/PackageCache/
[Ll]ibrary/ShaderCache/

# Except these files
!/[Ll]ibrary/AssetImportState
!/[Ll]ibrary/BuildPlayer.prefs
!/[Ll]ibrary/EditorBuildSettings.asset
!/[Ll]ibrary/ExpandedItems.asset
!/[Ll]ibrary/ScriptMapper
!/[Ll]ibrary/SourceAssetDB
!/[Ll]ibrary/InspectorExpandedItems.asset
!/[Ll]ibrary/UnityEditorScreenSettings.asset
!/[Ll]ibrary/UnityEditorSettings.asset

# Asset meta data should only be ignored when the corresponding asset is also ignored
!/[Aa]ssets/**/*.meta

# Uncomment this line if you wish to ignore the asset store tools plugin
# [Aa]ssets/AssetStoreTools*

# Autogenerated file by Unity3D
[Tt]emp/
[Tt]emp/**

# Autogenerated file by Unity3D
[Oo]bj/
[Oo]bj/**

# Unity3D Cache directory
[Uu]ser[Ss]ettings/
[Uu]ser[Ss]ettings/**

# Unity3D Generated File
[Dd]ebug/

# Unity3D Generated File On Crash Reports
sysinfo.txt

# Builds
[Bb]uild/
[Bb]uilds/

# Packed addressables
/[Aa]ssets/[Aa]ddressableAssetsData/*/*.bin*
/[Aa]ssets/[Aa]ddressableAssetsData/*/*.hash
/[Aa]ssets/[Aa]ddressableAssetsData/*/*.json

# VS Code
.vscode/

# Rider
.idea/
*.sln.iml

# Mono Auto Generated Files
mono_crash.*

# OSX
.DS_Store
